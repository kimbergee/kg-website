{
  "name": "kg-website",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --no-source-map --watch src/scss/base.scss:public/css/base.min.css --style compressed",
    "build:sass": "sass --no-source-map src/scss/base.scss:public/css/base.min.css --style compressed",
    "uglify": "mkdir -p public/js && uglifyjs src/js/main.js -m -c -o public/js/main.min.js",
    "clean": "rimraf public",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all clean uglify build:sass --parallel watch:*",
    "build": "npm-run-all clean uglify build:*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.13",
    "uglify-js": "^3.13.9"
  }
}
